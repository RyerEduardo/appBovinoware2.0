<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.myapplication">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">

        <activity android:name=".SplashScreen">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <activity
            android:name=".MainActivity"
            android:label="@string/cabecalio"
            android:theme="@style/AppTheme.NoActionBar" />

        <activity
            android:name=".MainCompras"
            android:label="@string/cabecalioCadastro1"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity
            android:name=".CadastrarCompra"
            android:label="@string/cabecalioCadastro1"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity
            android:name=".AlterarCompra"
            android:label="@string/cabecalioAlteraCompra"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity
            android:name=".MainVendas"
            android:label="@string/cabecalioCadastro"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity
            android:name=".CadastrarVenda"
            android:label="@string/cabecalioCadastro"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity
            android:name=".AlterarVenda"
            android:label="@string/cabecalioAltera"
            android:theme="@style/AppTheme.NoActionBar" />

    </application>

</manifest>